<body>
  <div class="container">
    <h1>TransitAlert</h1>

    <div id="lines" role="tablist" aria-multiselectable="true">
      <% lines_with_incidents.each do |line_name, line_data| %>
        <%= erb :'partials/line', locals: { line_name: line_name, line: line_data } %>
      <% end %>
    </div>
  </div>
</body>
